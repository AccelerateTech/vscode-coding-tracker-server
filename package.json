{
  "name": "vscode-coding-tracker-server",
  "version": "0.4.1",
  "description": "Visual Studio Code Coding Tracker Server Script",
  "main": "app.js",
  "scripts": {
    "test": "mocha test/src/*.js",
    "test-no-eslint-tsc": "mocha test/src/*.js --no-eslint --no-tsc",
    "build": "node ./build/build.js",
    "build-live": "node ./build/build.js -w",
    "start": "node ./app",
    "server": "node ./app",
    "dev-test-server": "node ./app -p 8080 -o ~/.coding-tracker",
    "install-git-hooks": "cp ./git-pre-commit-hook ./.git/hooks/pre-commit -f"
  },
  "bin": {
    "coding-tracker-server": "app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hangxingliu/vscode-coding-tracker-server"
  },
  "keywords": [
    "vscode",
    "server",
    "express",
    "analyzer"
  ],
  "dependencies": {
    "express": "*",
    "commander": "*",
    "colors": "*",
    "fs-extra": "*",
    "morgan": "*",
    "body-parser": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/colors": "*",
    "@types/mocha": "*",
    "@types/jquery": "*",
    "@types/bootstrap": "*",
    "@types/commander": "*",
    "@types/fs-extra": "*",
    "@types/express": "*",
    "@types/morgan": "*",
    "@types/body-parser": "*",
    "mocha": "*",
    "request": "*",
    "tree-kill": "*",
    "async": "*",
    "eslint": "*",
    "babel-eslint": "*",
    "typescript": "*"
  },
  "author": "hangxingliu",
  "license": "GPL-3.0"
}
